local keys = {"bypass", "uloltangabobo", "uloltangabobo"} -- key here
local userkey = ""

local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("Balli Hood Gui Key System", "DarkTheme")
local Tab = Window:NewTab("Key System")
local Section = Tab:NewSection("Key System")

Section:NewTextBox("Enter Key Here", "TextboxInfo", function(txt)
    userkey = txt -- Update the userkey variable with the entered key
end)

Section:NewButton("Check Key", "ButtonInfo", function()
    local isKeyCorrect = false
    for i, v in pairs(keys) do
        if userkey == v then
            isKeyCorrect = true
            break
        end
    end

    if isKeyCorrect then
for i,v in pairs(game.CoreGui:GetDescendants()) do
 if v.Name == "Main" then
  v:Destroy()
 end
end
    local Sound = Instance.new("Sound",game:GetService("SoundService"))
Sound.SoundId = "rbxassetid://3094993009"
Sound:Play()
-- [[ fnl example ]] --

-- Import the Library
local fnl = loadstring(game:HttpGetAsync 'https://raw.githubusercontent.com/Code1Tech/utils/main/notification.lua')()

-- Make a Notification function
function notify(title, text, duration)
  title = title or "Notification"
  text = text or "No text provided."
  duration = duration or 5
  
  fnl:MakeNotification({
    Title = title,
    Text = text,
    Duration = duration
  })
end

-- Use the func
notify("Key System", "Checking The Key...", 3)
wait(1)      
