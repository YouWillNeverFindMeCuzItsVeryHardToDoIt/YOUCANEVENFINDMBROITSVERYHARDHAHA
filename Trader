local gui = Instance.new("ScreenGui")
gui.Name = "RobloxGUI"
gui.Parent = game.CoreGui
gui.Parent = game.Players.LocalPlayer.PlayerGui

local changeframename = Instance.new("Frame")
changeframename.Size = UDim2.new(0, 450, 0, 250)
changeframename.Position = UDim2.new(0, 200, 0, 10)
changeframename.BackgroundColor3 = Color3.new(0, 0, 0)
changeframename.BorderColor3 = Color3.new(255, 0, 0)
changeframename.BorderSizePixel = 1
changeframename.Active = true
changeframename.Draggable = true
changeframename.Parent = gui

local textButton = Instance.new("TextButton")
textButton.Text = "Puzzle Key"
textButton.Size = UDim2.new(0, 70, 0, 50) -- Adjusted size to match the image
textButton.Position = UDim2.new(0.5, 150, 0.6, -25) -- Adjusted position to match the image
textButton.BorderColor3 = Color3.new(255, 0, 0)
textButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
textButton.TextColor3 = Color3.fromRGB(255, 255, 255)
textButton.Font = Enum.Font.SourceSans
textButton.FontSize = Enum.FontSize.Size24 -- Adjusted font size to match the image
textButton.BorderSizePixel = 1
textButton.Active = true
textButton.Draggable = false
textButton.Parent = changeframename
textButton.TextSize = 16



local function dragify(Button)
    local dragToggle = nil
    local dragSpeed = 0.25 -- You can edit this.
    local dragInput = nil
    local dragStart = nil
    local startPos = nil

    local function updateInput(input)
        local Delta = input.Position - dragStart
        local Position = UDim2.new(
            startPos.X.Scale,
            startPos.X.Offset + Delta.X,
            startPos.Y.Scale,
            startPos.Y.Offset + Delta.Y
        )
        game:GetService("TweenService"):Create(Button, TweenInfo.new(0.25), {Position = Position}):Play()
    end

    Button.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragToggle = true
            dragStart = input.Position
            startPos = Button.Position

            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragToggle = false
                end
            end)
        end
    end)

    
    game:GetService("UserInputService").InputChanged:Connect(function(input)
        if input == dragInput and dragToggle then
            updateInput(input)
        end
    end)
end

dragify(textButton)

textButton.MouseButton1Click:Connect(function()
    -- Add your text button script here
    local args = {
    [1] = "Puzzle Key"
}

game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("BuyItemCash"):FireServer(unpack(args))
end)

---------------------------------

